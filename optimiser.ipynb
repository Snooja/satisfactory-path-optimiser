    "# converting the above into a re-usable function\n",
    "\n",
    "# TODO split out the update target function to a different function, it won't be called at the same time in the parent loop\n",
    "\n",
    "def update(shell, target: str, target_rate):\n",
    "    '''\n",
    "    This function takes input target resource and target rate.\n",
    "    Updates these values in the solution data frame as needed.\n",
    "    Looks up how to make them.\n",
    "    Updates these as needed in the solution data frame.\n",
    "    Updated the target resource to target need = have.\n",
    "    '''\n",
    "    # set solution dataframe values to show we now need more than we have\n",
    "    shell.loc[target, 'need'] += target_rate\n",
    "    \n",
    "    # lookup what building and it's produciong rates will create this needed resource\n",
    "    row = df.loc[df.recipe == target]\n",
    "    \n",
    "    # check how many multiples of this production line we'll need\n",
    "    k = target_rate / row.produced[1]\n",
    "    \n",
    "    # lookup input resource 1 required to make target and multiply by k \n",
    "    resource1 = row.ingredient1.iloc[0]\n",
    "    want1 = row.consumed1.iloc[0] * k\n",
    "\n",
    "    shell.loc[resource1,'need'] += want1\n",
    "    shell.loc[target,'have'] = 0\n",
    "    \n",
    "    resource2 = row.ingredient2.iloc[0]\n",
    "    #  handling for when there's only 1 ingredient\n",
    "    if (len(resource2) > 0):\n",
    "        want2 = row.consumed2.iloc[0] * k \n",
    "\n",
    "        shell.loc[resource2,'need'] += want2\n",
